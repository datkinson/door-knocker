<div class="container">
  <h1>{{title}}</h1>

  <table class="table">
  <thead>
    <tr>
        <th scope="col">Username</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
    {{#each users}}
      <tr>
          <th scope="row">{{username}}</th>
          <td>{{firstName}}</td>
          <td>{{lastName}}</td>
          <td>{{email}}</td>
      </tr>
    {{/each}}
  </tbody>
  </table>
  <a class="btn btn-primary" href="/users/register">Create User</a>
</div>